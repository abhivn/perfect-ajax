<% @blogs.each do |b| %>
<tr>
  <td><%= b.id %></td>
  <td><%= b.title %></td>
  <td><%= b.body %></td>
  <td><%= b.created_at %></td>
  <td><%= link_to "Edit", edit_blog_path(b), class: "btn btn-warning", remote: true %> <%= link_to "Delete", blog_path(b),method: :delete, data: {confirm: 'Are you sure to delete?'}, remote: true, class: "btn btn-danger" %></td>
</tr>
<% end %>
